{
  "version": 2,
  "name": "typewritesomething",
  "builds": [{
      "src": "package.json", 
      "use": "@now/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "./index.html",
      "use": "@now/static"
    },
    {
      "src": "./static/**",
      "use": "@now/static"
    },
    {
      "src": "./favicon.ico",
      "use": "@now/static"
    },
    {
      "src": "./dist/main.js",
      "use": "@now/static"
    }
  ],
  "github": {
    "autoAlias": false
  }
}
