{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "import/prefer-default-export": "off"
    },
    "globals": {
        "shake": "readonly"
    },
    "parser": "babel-eslint"
}
